__constant static const ulong WPL_RNDC[10] =
{
	0x4F01B887E8C62318UL, 0x52916F79F5D2A636UL,
	0x357B0CA38E9BBC60UL, 0x57FE4B2EC2D7E01DUL,
	0xDA4AF09FE5377715UL, 0x856BA0B10A29C958UL,
	0x67053ECBF4105DBDUL, 0xD8957DA78B4127E4UL,
	0x9E4717DD667CEEFBUL, 0x33835AAD07BF2DCAUL
};

#define BYTE(x, y)	(amd_bfe((uint)((x) >> ((y >= 32U) ? 32U : 0U)), (y) - (((y) >= 32) ? 32U : 0), 8U))

#define ROUND_ELT(in, i0, i1, i2, i3, i4, i5, i6, i7)	(T[(in.s ## i0) & 0xFF] ^ rotate(T[BYTE(in.s ## i1, 8U)], 8UL) ^ rotate(T[BYTE(in.s ## i2, 16U)], 16UL) ^ rotate(T[BYTE(in.s ## i3, 24U)], 24UL) ^ \
														as_ulong(as_uint2(T[BYTE(in.s ## i4, 32U)]).s10) ^ rotate(T[BYTE(in.s ## i5, 40U)], 40UL) ^ rotate(T[BYTE(in.s ## i6, 48U)], 48UL) ^ rotate(T[(in.s ## i7) >> 56], 56UL))

ulong8 WhirlpoolRound(ulong8 st, ulong8 key, const __local ulong *T)
{
	#if defined(__Tahiti__) || defined(__Pitcairn__)
	#pragma unroll 5
	#else
	#pragma unroll 1
	#endif
	for(int i = 0; i < 10; ++i)
	{
		ulong8 tmp;
		
		// Calculate round key from previous round key
		tmp.s0 = ROUND_ELT(key, 0, 7, 6, 5, 4, 3, 2, 1) ^ WPL_RNDC[i];
		tmp.s1 = ROUND_ELT(key, 1, 0, 7, 6, 5, 4, 3, 2);
		tmp.s2 = ROUND_ELT(key, 2, 1, 0, 7, 6, 5, 4, 3);
		tmp.s3 = ROUND_ELT(key, 3, 2, 1, 0, 7, 6, 5, 4);
		tmp.s4 = ROUND_ELT(key, 4, 3, 2, 1, 0, 7, 6, 5);
		tmp.s5 = ROUND_ELT(key, 5, 4, 3, 2, 1, 0, 7, 6);
		tmp.s6 = ROUND_ELT(key, 6, 5, 4, 3, 2, 1, 0, 7);
		tmp.s7 = ROUND_ELT(key, 7, 6, 5, 4, 3, 2, 1, 0);
		
		// Stuff round key in key var
		key = tmp;
		
		// Run rounds again on the state
		tmp.s0 = ROUND_ELT(st, 0, 7, 6, 5, 4, 3, 2, 1);
		tmp.s1 = ROUND_ELT(st, 1, 0, 7, 6, 5, 4, 3, 2);
		tmp.s2 = ROUND_ELT(st, 2, 1, 0, 7, 6, 5, 4, 3);
		tmp.s3 = ROUND_ELT(st, 3, 2, 1, 0, 7, 6, 5, 4);
		tmp.s4 = ROUND_ELT(st, 4, 3, 2, 1, 0, 7, 6, 5);
		tmp.s5 = ROUND_ELT(st, 5, 4, 3, 2, 1, 0, 7, 6);
		tmp.s6 = ROUND_ELT(st, 6, 5, 4, 3, 2, 1, 0, 7);
		tmp.s7 = ROUND_ELT(st, 7, 6, 5, 4, 3, 2, 1, 0);
		
		// Mix key with state
		st = tmp ^ key;
	}
	
	return(st);
}
